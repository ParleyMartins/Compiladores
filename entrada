inicio
	inteiro a <- 4 + 12
	inteiro b <- (10 - 20)
	inteiro c

	inteiro a
	inteiro b
	booleano boo <- verdadeiro

	se  boo entao
		escreva "testando"
	fim_se

	a <- 10

	inteiro teste
	teste <- "10+20ads?!@#w [] {}"

	se verdadeiro entao
		escreva "teste"	
	fim_se

	c <- a+b
	escreva c

	para x de 2 ate 5
		escreva "Hello"
	fim_para
	
	enquanto c>0 
		escreva "While"
		c <- 0
	fim_enquanto

	escreva c

	se a>10 entao
		escreva "Oi"
	fim_se
fim


